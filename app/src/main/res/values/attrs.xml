<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ImageEditorView">
        <attr name="iev_cornerRadius" format="dimension" />
        <attr name="iev_borderWidth" format="dimension" />
        <attr name="iev_borderColor" format="color" />
    </declare-styleable>

    <declare-styleable name="LayerPreviewImageView">
        <attr name="lpiv_borderWidth" format="dimension" />
        <attr name="lpiv_borderColor" format="color" />
        <attr name="lpiv_cornerRadius" format="dimension" />
        <attr name="lpiv_text" format="string" />
        <attr name="lpiv_textSize" format="dimension" />
        <attr name="lpiv_textColor" format="color" />
        <attr name="lpiv_layerType" format="enum">
            <enum name="image" value="0" />
            <enum name="background" value="1" />
            <enum name="text" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="EditorSaveView">
        <attr name="esv_borderWidth" format="dimension" />
        <attr name="esv_borderColor" format="color" />
        <attr name="esv_cornerRadius" format="dimension" />
        <attr name="esv_saveText" format="string" />
        <attr name="esv_textSize" format="dimension" />
        <attr name="esv_textColor" format="color" />
        <attr name="esv_saveIcon" format="reference" />
        <attr name="esv_galleryText" format="string" />
        <attr name="esv_settingText" format="string" />
        <attr name="esv_settingIcon" format="reference" />
        <attr name="esv_arrowIcon" format="reference" />
        <attr name="esv_iconTint" format="color" />
        <attr name="esv_hPadding" format="dimension" />
        <attr name="esv_vPadding" format="dimension" />
        <attr name="esv_isAutoSaveMode" format="boolean" />
        <attr name="esv_progressMaskColor" format="color" />
        <attr name="esv_progressMaskDuration" format="color" />
        <attr name="esv_iconSize" format="dimension" />
        <attr name="esv_doneSize" format="dimension" />
        <attr name="esv_doneIcon" format="reference" />
        <attr name="esv_progressDoneColor" format="color" />
        <attr name="esv_progressDoneDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="SaveLoadingButton">
        <attr name="slb_borderWidth" format="dimension" />
        <attr name="slb_borderColor" format="color" />
        <attr name="slb_bgColor" format="color" />
        <attr name="slb_cornerRadius" format="dimension" />
        <attr name="slb_startIcon" format="reference" />
        <attr name="slb_startIconTint" format="color" />
        <attr name="slb_iconSize" format="dimension" />
        <attr name="slb_iconMargin" format="dimension" />
        <attr name="slb_saveText" format="string" />
        <attr name="slb_textSize" format="dimension" />
        <attr name="slb_textColor" format="color" />
        <attr name="slb_shimmerColor" format="color" />
        <attr name="slb_shimmerDuration" format="integer" />
        <attr name="slb_shimmerWidth" format="dimension" />
        <attr name="slb_saveState" format="enum">
            <enum name="saving" value="0" />
            <enum name="done" value="1" />
        </attr>
        <attr name="slb_hPadding" format="dimension" />
        <attr name="slb_vPadding" format="dimension" />
    </declare-styleable>
</resources>